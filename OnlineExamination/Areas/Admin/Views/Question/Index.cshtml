@model IEnumerable<Questions>
@{
    ViewData["Title"] = "Index";
}
<div class=" container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">All Questions List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Exam" asp-action="Index" class="btn btn-primary">
                <i class="bi bi-list-ul"></i>&nbsp; Exam list
            </a>
        </div>
    </div>
    </br>
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Exam Name
                </th>
                <th>
                    Question
                </th>
                <th>
                    Marks per question
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="30%">
                        @obj.ExamDetails.Name
                    </td>

                    <td width="30%">
                        @obj.Ques
                    </td>
                    <td width="15%">
                        @obj.MarksPerQues
                    </td>
                  
                </tr>
            }
        </tbody>
    </table>
</div>

